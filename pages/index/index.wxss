.boox-top {
    padding: 20rpx;
}

.management-left,
.right-img {
    width: 75rpx;
    height: 75rpx;
    line-height: 86rpx;
}

.management-right {
    width: 230rpx;
}

.img-size {
    width: 44rpx;
    height: 44rpx;
}

.hlep {
    width: 126rpx;
    height: 75rpx;
    line-height: 75rpx;
    border: 2rpx solid #999999;
    border-radius: 26rpx;
}

.no-boox {
    margin-top: 20rpx;
    font-size: 34rpx;
}

.collect {
    padding: 20rpx;
}

.collect-box {
    width: 30%;
    margin-left: 3%;
    margin-bottom: 20rpx;
}

.collect-img {
    width: 100%;
    height: 296rpx;
}

.collectImg-size {
    width: 100%;
    height: 296rpx;
}

.collect-title {
    font-size: 34rpx;
    padding: 10rpx 0 6rpx 0;
}

.collect-chapter {
    font-size: 32rpx;
    color: #999999;
}

.closeImg {
    width: 50rpx;
    height: 50rpx;
    right: 0;
    z-index: 1;
}

.closeImg-size {
    width: 50rpx;
    height: 50rpx;
}

.jitter {
    animation: viewlinear 1.5s linear infinite;
}

@-webkit-keyframes viewlinear {
    0% {
        transform: rotate(0deg);
    }
    25% {
        transform: rotate(4deg);
    }
    50% {
        transform: rotate(0deg);
    }
    75% {
        transform: rotate(-3deg);
    }
    100% {
        transform: rotate(0deg);
    }
    /* from {
        transform: rotate(0deg);
    }
    to {
        transform: rotate(4deg);
    } */
}